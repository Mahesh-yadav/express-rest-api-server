{
  "env": {
    "es2020": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-plusplus": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "comma-dangle": "off"
  }
}
